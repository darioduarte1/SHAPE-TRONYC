[MASTER]
load-plugins=pylint_django
django-settings-module=backend.settings

[MESSAGES CONTROL]
disable=
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring,
    invalid-name,
    no-member

[FORMAT]
max-line-length=100

[BASIC]
good-names=i,
           j,
           k,
           ex,
           Run,
           _

[TYPECHECK]
generated-members=UserProfile.objects
